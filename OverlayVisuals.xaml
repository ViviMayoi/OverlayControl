<Window x:Class="OverlayControl.OverlayVisuals"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OverlayControl"
        Closing="wndCutIn_Closing" x:Name="wndCutIn" Title="OverlayControl Visuals" Height="768" Width="1030"
        ResizeMode="NoResize" Background="Lime">
    <Grid>
        <Image x:Name="imgCutIn1" Height="512" Stretch="Uniform" VerticalAlignment="Top" HorizontalAlignment="Left" Width="512" />
        <Image x:Name="imgCutIn2" Height="512" Stretch="Uniform" VerticalAlignment="Top" HorizontalAlignment="Right" Width="512" />
        <Image x:Name="imgFlag1" Height="227" Stretch="Uniform" VerticalAlignment="Top" Margin="42,512,0,0" HorizontalAlignment="Left" Width="470" />
        <Image x:Name="imgFlag2" Height="227" Stretch="Uniform" VerticalAlignment="Top" Margin="0,512,42,0" HorizontalAlignment="Right" Width="470" />
        <Image x:Name="imgMoon1" HorizontalAlignment="Left" Width="42" Height="42" VerticalAlignment="Bottom"/>
        <Image x:Name="imgMoon2" HorizontalAlignment="Right" Width="42" Height="42" VerticalAlignment="Bottom"/>
    </Grid>
</Window>