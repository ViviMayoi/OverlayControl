<Window x:Class="OverlayControl.CharacterCutIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OverlayControl"
        Closing="wndCutIn_Closing" x:Name="wndCutIn" Title="CharacterCutIn" Height="512" Width="512"
        ResizeMode="NoResize" Background="Lime">
  <Grid>
    <Image x:Name="imgCutIn" Margin="0,0,0,0" Stretch="Uniform"/>
    <Image x:Name="imgMoon" HorizontalAlignment="Left" Width="42" Height="42" VerticalAlignment="Bottom"/>
  </Grid>
</Window>